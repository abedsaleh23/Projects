    <div class="header">
        <div class="header-top">
            <div class="container">
                <div class="head-top">
                    <div class="logo">
                        <h1><a href="index.html"> Become a  <span>Coach</span></a></h1>
                    </div>
                    <div class="top-nav">
                        <span class="menu"><img src="/images2/menu.png" alt=""> </span>
                        <ul>
                            <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                            <li class="active"><a asp-area="" asp-controller="Home" asp-action="Games">Games</a></li>
                            <li><a asp-area="" asp-controller="Home" asp-action="About">About</a></li>
                            <li><a asp-area="" asp-controller="Home" asp-action="Contact">Contact</a></li>
                            <li>
                                <form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Page("/", new { area = "" })" method="post">
                                    <button style="color:#ffffff" type="submit" class="nav-link btn btn-link text-dark">Logout</button>
                                </form>
                            </li>

                            <div class="clearfix"></div>
                        </ul>


                        <!--script-->
                        <script>
                            $("span.menu").click(function () {
                                $(".top-nav ul").slideToggle(500, function () {
                                });
                            });
                        </script>

                    </div>

                    <div class="clearfix"> </div>
                </div>
            </div>
        </div>
    </div>

