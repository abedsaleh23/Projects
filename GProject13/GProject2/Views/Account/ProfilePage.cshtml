                <div class="advn-two">
                    <h4 style="color:#cad0f6"> coach statement</h4>
                    <p>@Html.DisplayFor(model => model.About)</p>
                    <ul style="color:#cad0f6">
                        <li><i class="glyphicon glyphicon-ok"></i>acheivment 1</li>
                        <li><i class="glyphicon glyphicon-ok"></i>acheivment 2</li>
                        <li><i class="glyphicon glyphicon-ok"></i>acheivment 3</li>
                        <li><i class="glyphicon glyphicon-ok"></i>acheivment 4</li>
                        <li><i class="glyphicon glyphicon-ok"></i>acheivment 5</li>
                    </ul>
                </div>
            </div>




            <div class="clearfix"></div>
        </div>


        <!--coach card-->
        <!--booking card-->

        <!--booking card-->
